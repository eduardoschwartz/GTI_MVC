    <style>
        .center {
            margin: auto;
        }

        body:lang(en) {
            margin-top: 1em;
        }
        body {
            margin: 1em 8% 1em 8%;
            margin-top: 2em;
            min-width: 900px;
            position: relative;
            box-shadow: 0 .3em 1em #000;
            border-radius: 13px;
        }

        #header {
            position: relative;
            z-index: 9;
            height: 60px;
            top: 0;
            left: 0;
            right: 0;
            text-align: center;
            display: block;
            border-radius: 13px 13px 0px 0px;
            background: linear-gradient(to bottom,darkred,black);
            display: flex;
            justify-content: center;
        }

        #footer {
            position: fixed;
            height: 30px;
            bottom: 0;
            left: 0;
            right: 0;
            text-align: center;
            display: block;
            background: #222222;
            padding-top: 5px;
        }

        #container {
            margin: 0 auto;
            overflow: auto;
            padding: 80px 0;
            width: auto;
        }

        #content {
        }

        .footer {
            background: red;
            position: relative;
            left: 0;
            right: 0;
            bottom: 0;
            height: 50px;
        }

        .divider {
            border-block-start: 1px solid gray;
            margin-top:3px;
            height:6px;
        }
    </style>

</head>
