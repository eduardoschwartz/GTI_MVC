@{
    Layout = "~/Views/Shared/_Layout.cshtml";
    ViewData["Title"] = "Menu de Opções";
}
<link href="~/Content/gti.css" rel="stylesheet" />
<br />
<br />
<div style="width: 100%; overflow: hidden;">
    <div style="width: 400px; float: left;">
        <span style="color:saddlebrown;font-weight:bold;font-style:italic">Para seu imóvel</span><br />
        <a href="http://sistemas.jaboticabal.sp.gov.br/gti/Pages/SegundaViaIPTU.aspx?d=gti">Emissão de 2ª via do carnê de IPTU (2020)</a><br />
        @*<a href="http://sistemas.jaboticabal.sp.gov.br/gti/Pages/dadosImovel.aspx?d=gti">Dados cadastrais de um imóvel</a><br />*@
        @Html.ActionLink("Dados cadastrais de um imóvel", "Dados_Imovel", "Imovel")<br />
        @Html.ActionLink("Certidão de endereço atualizado", "Certidao_Endereco", "Imovel")<br />
        @Html.ActionLink("Certidão de valor venal", "Certidao_Valor_Venal", "Imovel")<br />
        @Html.ActionLink("Certidão de isenção de IPTU", "Certidao_Isencao", "Imovel")<br />
    </div>

    <div style="margin-left: 420px;">
        <span style="color:saddlebrown;font-weight:bold;font-style:italic">Para sua empresa</span><br />
        @Html.ActionLink("Dados cadastrais de uma empresa", "Details", "Empresa")<br />
        <a href="~/Files/DECA2018.pdf">Download da declaração cadastral (DECA)</a><br />
        <a href="http://sistemas.jaboticabal.sp.gov.br/gti/Pages/alvara_funcionamento.aspx?d=gti">Renovação de alvará de funcionamento</a><br />
        @Html.ActionLink("Comprovante de inscrição e situação cadastral", "Certidao_Inscricao", "Empresa")<br />
        @Html.ActionLink("Situação dos débitos ref. a ISS Fixo, Taxa de Licença e Vig. Sanitária", "Certidao_Pagamento", "Empresa")
    </div>
</div>


<br />
<br />
<br />
<div style="width: 100%; overflow: hidden;">
    <div style="width: 400px; float: left;">
        <span style="color:saddlebrown;font-weight:bold;font-style:italic">Operações financeiras</span><br />
        @Html.ActionLink("Consulta e atualização de boletos para pagamento", "Dama", "Tributario")<br />
        @*<a href="http://sistemas.jaboticabal.sp.gov.br/gti/Pages/gtiMenu2.aspx?d=gti" style="width: 600px">Consulta e atualização de boletos vencidos para pagamento</a>*@
        @Html.ActionLink("Certidão de débito (Código do imóvel ou inscrição municipal)", "Certidao_Debito_Codigo", "Tributario")<br />
        @Html.ActionLink("Certidão de débito (CPF/CNPJ)", "Certidao_Debito_Doc", "Tributario")<br />
        @Html.ActionLink("Impressão de comprovante de pagamento", "Comprovante_pagamento", "Tributario")<br />
        <a href="http://sistemas.jaboticabal.sp.gov.br/gti/Pages/detalhe_boleto.aspx?d=gti">Impressão dos detalhes de um boleto</a><br />
        <a href="LiberacaoParc.aspx?d=gti" style="width: 600px">Emissão de carnê de parcelamento (parcelas de 2020)</a>
    </div>

    <div style="margin-left: 420px;">
        <span style="color:saddlebrown;font-weight:bold;font-style:italic">Outras opções</span><br />
        <a href="http://sistemas.jaboticabal.sp.gov.br/gti/Pages/ConsultaProcesso.aspx?d=gti">Consulta de Processos</a><br />
        @Html.ActionLink("Acesso ao Sistema", "Login", "Home")
    </div>
</div>

